.PARAM  ndi=1        Rdi=11m     nmu2=0       ta=40n       td=10n
.PARAM  Rf=0.27

.PARAM  f1=374p      f2=253p     f3=200p      U0=684m      bt=2.4u
.PARAM  k10=184n     k11=1u      eps=0.01     dV=-1        e1=-749m
.PARAM  k6=23.3f     k3=595p     g1=7.132     k4=-40.5Meg  k5=38.4
